[user]
	email = svensson.joakim@gmail.com
	name = Joakim Svensson
[apply]
	whitespace = nowarn
[push]
	default = tracking
[alias]
	br = branch
	b  = branch
	st = status
	co = checkout
	d  = difftool
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
[color]
	ui = auto
[color "diff"]
	meta = yellow bold
	frag = cyan
	old = red
	new = green bold
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	tool = vimdiff
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[instaweb]
	browser = iceweasel
	httpd = apache2
	local = true
[github]
	user = jokkemann
[url "git@github.com:"]
	pushInsteadOf = "git://github.com/"
[core]
	editor = /usr/bin/vim
